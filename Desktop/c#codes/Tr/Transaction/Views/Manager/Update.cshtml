@model Transaction.ResponseModel.ManagerResponse;
<p style="color: red; font-weight: bold;">Update a manager by filling out the form below</p>
<form method="Post">
    <div class="col-md-6" style = "margin-top:40px; font-size: larger; font -weight: bold;colour: darkcyan">
        <label value="FirstName" class= "form-label">FirstName</label>
        <input value="@Model.Data.FirstName" value = "FirstName" type="text" class="form-control">
        <label value="LastName" class= "form-label">LastName</label>
        <input value="@Model.Data.FirstName" value = "LastName" type="text" class="form-control">
        <label value="Email" class= "form-label"></label>
        <input value="@Model.Data.Email" value = "Email" type="text" class="form-control">
    </div>
    <button type="submit">Update</button>
</form>